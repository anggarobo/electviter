{
  "name": "electviter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-main/main.js",
  "author": "anggarobo",
  "license": "MIT",
  "description": "A Electron app with React and Vite",
  "scripts": {
    "transpile:main": "tsc -p config/tsconfig.base.json",
    "dev": "concurrently \"pnpm dev:ui\" \"pnpm dev:main\"",
    "dev:ui": "vite",
    "dev:main": "pnpm run transpile:main; cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:linux": "pnpm run transpile:main && pnpm run build && electron-builder --linux --x64",
    "dist:mac": "pnpm run transpile:main && pnpm run build && electron-builder --mac --arm64",
    "dist:win": "pnpm run transpile:main && pnpm run build && electron-builder --win --x64",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "electron-context-menu": "^4.1.0",
    "fs-extra": "^11.3.0",
    "image-size": "^2.0.2",
    "os-utils": "^0.0.14",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.0.0",
    "winattr": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@mantine/core": "^8.1.2",
    "@mantine/hooks": "^8.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/os-utils": "^0.0.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/winattr": "^3.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "pnpm": {
    "neverBuiltDependencies": []
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
